{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "android": "expo start --android",
    "build": "yarn clean && yarn tsc",
    "clean": "rimraf build",
    "start": "concurrently \"expo start -c\" \"yarn watch\"",
    "start:tunnel": "concurrently \"expo start --tunnel -c\" \"yarn watch\"",
    "eject": "expo eject",
    "ios": "expo start --ios",
    "lint": "tslint --project .",
    "snapshot": "yarn test --updateSnapshot",
    "test": "node ./node_modules/jest/bin/jest.js",
    "test:ci": "yarn build && yarn lint && yarn test",
    "coverage": "yarn test --coverage",
    "tsc": "tsc",
    "watch": "yarn build -w"
  },
  "dependencies": {
    "@expo/samples": "2.1.1",
    "expo": "^32.0.0",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-native-elements": "^1.1.0",
    "react-navigation": "^3.0.9"
  },
  "devDependencies": {
    "@types/expo": "^32.0.3",
    "@types/jest": "^24.0.9",
    "@types/mockdate": "^2.0.0",
    "@types/react": "^16.8.5",
    "@types/react-dom": "^16.8.2",
    "@types/react-native": "^0.57.38",
    "@types/react-navigation": "^3.0.4",
    "@types/react-test-renderer": "^16.8.1",
    "babel-preset-expo": "^5.0.0",
    "concurrently": "^4.1.0",
    "expo-cli": "^2.11.9",
    "jest": "^24.1.0",
    "jest-expo": "^32.0.0",
    "mockdate": "^2.0.2",
    "react-test-renderer": "^16.8.3",
    "rimraf": "^2.6.3",
    "tslint": "^5.13.0",
    "typescript": "^3.3.3333"
  },
  "private": true
}
