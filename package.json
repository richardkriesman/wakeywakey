{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "build": "yarn clean && yarn tsc",
    "build:android": "yarn build && yarn expo build:android --release-channel dev",
    "build:clean": "yarn clean && yarn tsc",
    "build:docs": "typedoc src --options typedoc.js",
    "build:ios": "yarn build && yarn expo build:ios --release-channel dev",
    "clean": "rimraf build",
    "serve-docs": "python -m http.server 3000",
    "start": "concurrently --restart-tries 100 -i --default-input-target 0 \"expo start -c\" \"yarn watch\"",
    "start:android": "expo start --android",
    "start:ios": "expo start --ios",
    "start:tunnel": "concurrently \"expo start --tunnel -c\" \"yarn watch\"",
    "eject": "expo eject",
    "lint": "tslint --project .",
    "release:android": "yarn build && yarn expo build:android",
    "release:ios": "yarn build && yarn expo build:ios",
    "snapshot": "yarn test --updateSnapshot",
    "test": "node ./node_modules/jest/bin/jest.js --silent",
    "test:ci": "yarn build:clean && yarn lint && yarn test --coverage",
    "coverage": "yarn test --coverage",
    "coveralls": "coveralls < coverage/lcov.info",
    "tsc": "tsc",
    "watch": "yarn build -w"
  },
  "dependencies": {
    "@expo/samples": "2.1.1",
    "@expo/vector-icons": "^10.0.1",
    "expo": "^32.0.0",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-native-elements": "^1.1.0",
    "react-native-vertical-swipe": "^1.1.1",
    "react-native-modal": "^9.0.0",
    "react-navigation": "^3.0.9"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@types/expo": "^32.0.3",
    "@types/jest": "^24.0.9",
    "@types/mockdate": "^2.0.0",
    "@types/react": "^16.8.5",
    "@types/react-dom": "^16.8.2",
    "@types/react-native": "^0.57.38",
    "@types/react-navigation": "^3.0.4",
    "@types/react-test-renderer": "^16.8.1",
    "@types/typescript": "^2.0.0",
    "babel-preset-expo": "^5.0.0",
    "concurrently": "^4.1.0",
    "coveralls": "^3.0.3",
    "expo-cli": "^2.14.0",
    "jest": "^24.1.0",
    "jest-expo": "^32.0.0",
    "mockdate": "^2.0.2",
    "react-test-renderer": "^16.8.3",
    "rimraf": "^2.6.3",
    "tslint": "^5.13.0",
    "typedoc": "^0.14.2",
    "typedoc-plugin-external-module-name": "^2.0.0",
    "typescript": "^3.4.3"
  },
  "private": true
}
